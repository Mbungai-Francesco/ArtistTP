{
  "version": 2,
  "builds": [
    { "src": "dist/src/server.js"}
  ],
  "routes": [
    { "src": "/api/artists(.*)", "dest": "dist/src/server.js" },
    { "src": "/api/users(.*)", "dest": "dist/src/server.js" },
    { "src": "/api/ratings(.*)", "dest": "dist/src/server.js" },
    { "src": "/api/login(.*)", "dest": "dist/src/server.js" },
    { "src": "/api/artist_ratings(.*)", "dest": "dist/src/server.js" },
    { "src": "/api/rate(.*)", "dest": "dist/src/server.js" },
    { "src": "/(.*)", "dest": "dist/src/server.js" }
  ]
}