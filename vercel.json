{
  "version": 2,
  "builds": [
    { "src": "src/server.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/artists(.*)", "dest": "src/server.ts" },
    { "src": "/api/users(.*)", "dest": "src/server.ts" },
    { "src": "/api/ratings(.*)", "dest": "src/server.ts" },
    { "src": "/api/login(.*)", "dest": "src/server.ts" },
    { "src": "/api/artist_ratings(.*)", "dest": "src/server.ts" },
    { "src": "/api/rate(.*)", "dest": "src/server.ts" },
    { "src": "/(.*)", "dest": "src/server.ts" }
  ]
}